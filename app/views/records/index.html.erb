<p id="notice"><%= notice %></p>

<h1>Records</h1>

<table>


 
    <% @subjects.each do |subject| %>
  <thead>
    <tr>
      <td><h4>Subject: <%= subject.name + "("+subject.subjectcode + ")" %></h4></td>
    </tr>
    <tr>
      
      <th>StudentID</th>
      <th>StudentName</th>      
      <th>SubjectName</th>
       <th>Obtmarks</th>
       <th>Totmarks</th>

      <th colspan="3"></th>
    </tr>

  </thead>


    <% @students.each do |student| %>
 
      <%= render 'form', record: @record,student: student, subject: subject%>

    <% end %>




    <% end %>



</table>







